{
    "name": "obsidian-git",
    "version": "2.19.1",
    "description": "Backup your Obsidian (https://obsidian.md) vault with git",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs dev",
        "build": "node esbuild.config.mjs production",
        "release": "standard-version",
        "lint": "eslint --ext .ts,.svelte src",
        "format": "prettier --check src",
        "prepare": "husky install"
    },
    "lint-staged": {
        "*.{js,ts,svelte}": [
            "npx eslint --fix",
            "npx prettier --write"
        ],
        "*.ts": [
            "npx tsc --noEmit"
        ]
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "standard-version": {
        "t": ""
    },
    "devDependencies": {
        "@codemirror/state": "^6.2.0",
        "@codemirror/view": "^6.9.2",
        "@types/debug": "^4.1.7",
        "@types/diff": "^5.0.2",
        "@types/hogan.js": "^3.0.1",
        "@types/node": "^14.18.38",
        "@typescript-eslint/eslint-plugin": "5.29.0",
        "@typescript-eslint/parser": "5.29.0",
        "esbuild": "^0.17.12",
        "esbuild-svelte": "^0.7.3",
        "eslint": "^8.36.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "husky": "^8.0.3",
        "lint-staged": "^13.2.1",
        "obsidian": "^0.16.3",
        "prettier": "2.8.4",
        "sass": "^1.59.2",
        "scss": "^0.2.4",
        "standard-version": "^9.5.0",
        "svelte": "^3.56.0",
        "svelte-check": "^3.1.4",
        "svelte-preprocess": "^5.0.3",
        "tslib": "^2.5.0",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "buffer": "^6.0.3",
        "debug": "^4.3.4",
        "diff": "^5.1.0",
        "diff2html": "^3.4.33",
        "isomorphic-git": "^1.22.0",
        "obsidian-community-lib": "^2.0.2",
        "simple-git": "Vinzent03/git-js#release",
        "supports-color": "^7.2.0"
    },
    "moduleFileExtensions": [
        "js",
        "ts",
        "svelte"
    ]
}
