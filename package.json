        "debug": "^4.3.4",
        "deep-equal": "^2.2.3",
        "diff": "^5.2.0",
        "diff2html": "^3.4.47",
        "isomorphic-git": "^1.25.3",
        "js-sha256": "^0.9.0",
        "obsidian-community-lib": "github:Vinzent03/obsidian-community-lib#upgrade",
